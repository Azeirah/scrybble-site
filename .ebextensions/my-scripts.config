container_commands:
    01-composer:
        command: "composer install -n --no-dev >> /var/log/eb-activity.log 2>&1"
    02-npm:
        command: "npm install >> /var/log/eb-activity.log 2>&1"
    03-js:
        command: "npm run prod"
    04-migration:
        command: "php artisan migrate >> /var/log/eb-activity.log 2>&1"
        leader_only: true
    05-optimization:
        command: "php artisan optimize >> /var/log/eb-activity.log 2>&1"
